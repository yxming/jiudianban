/* page/user/mine/mine.wxss */
page {
  background-color: #F0F0F3;
}

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-image: url('https://636c-cloud1-7go51v8te374de35-1318782235.tcb.qcloud.la/Myid.png?sign=e94059f4136def61c4155911bfdf2b55&t=1691031887');
  background-size: cover;
  background-repeat: no-repeat;
}


.profile {
  width: 100%;
  height: 100%;
  z-index: 900;
  /* position: relative; */
}
/* 顶部导航 */
.shangmbubox{
  width: 100%;
  height: 150rpx;
  position: relative;
  top: 0;
  left: 0;
}
.dohangind{
  width: auto;
  height: auto;
  text-align: center;
  font-size: 40rpx;
  font-weight: 600;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.profileback{
  width: 100%;
  height: 100vh;
  z-index: 1;
}
.myImage{
  width: 354rpx;
  height: 354rpx;
  margin: auto;
  margin-top: 100rpx;
  z-index: 99;
  overflow: hidden;
  position: relative;
}
.myimage{
  width: 70%;
  height: 70%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 50rpx;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 50%;
  overflow: hidden;
}
.nametext{
  width: 100%;
  height: 70rpx;
  z-index: 99;
  display: flex;
  margin-top: 150rpx;
}
.sayHello{
  width: 50%;
  height: 100%;
  color: #00754A;
  font-style: 70rpx;
}
.riceBagBorder{
  width: 380rpx;
  height:auto;
  z-index: 99;
  margin: auto;
  margin-top: 50rpx;
}
.riceBagDBorder{
  width: 100%;
  height: 110rpx;
  position: relative;
  z-index: 901;
}
.riceBagDBordertext{
  width: 300rpx;
  height: 50rpx;
  z-index: 999;
  position: absolute;
  top: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  text-align:center;
  color: #00754A;
  font-size: 32rpx;
  z-index: 9900;
}
/* 密码模态框 */
.block {
  position: relative;
  width: 240px;
  height: auto;
  border-radius: 16rpx;
  margin: 30rpx;
}
.input-box{
  width: 100%;
  height: 80rpx;
  text-align: center;
}



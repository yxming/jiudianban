page {
  background-color: #F0F0F3; /* 设置背景颜色#f0f0f0; */
}



.backgroundBox{
  width: 100%;
  height: 100vh;
  position: relative;
}

/* 顶部导航 */
.shangmbubox{
  width: 100%;
  height: 90rpx;
  background-color: #F0F0F3;
  position: relative;
  top: 0;
  left: 0;
  padding-top: 95rpx;
}
.dohangind{
  width: 300rpx;
  height: auto;
  margin: auto;
  text-align: center;
  font-size: 40rpx;
  font-weight: 600;
}
/* 上面用的流程图 */
.upperNavigation{
  width: 100%;
  height: 212rpx;
  margin-top: 30rpx;
  display:flex;
  justify-content:space-between;
}
.lliucheng{
  width: 268rpx;
  height: auto;
  background-color: salmon;
}



/* 图片的 */
.upimg{
  width: 100%;
  height: 60rpx;
}
.uimg{
  width: 100%;
  height: 100%;
}
/* 滑动块的 */
.slideBox{
width: calc(100%-20rpx);
height: 230rpx;
background-color: rgb(247, 247, 247);
padding: 20rpx;
border-radius: 30rpx;
font-weight: normal;
}
.slidesBox{
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
}
.slideabox{
  width: 200rpx;
  height: 50rpx;
  background-color: cornflowerblue;
  text-align: center;
  color: #fff;
}
.slidestext{
  width: 50%;
  height: auto;
}
.slideBzbox{
  width: 100%;
  height: 160rpx;
  display: flex;
  justify-content: space-between;
}
.slideBbox{
  width: 500rpx;
  height: 160rpx;
  background-color: darkblue;
}
.rslidsbox{
  width: 200rpx;
  height: 160rpx;
}
/* 下面按钮块 */
.danbox{
  width: 720rpx;
  height: 90rpx;
  background-color: darkcyan;
  border-radius: 30rpx;
  position: absolute;
  left: 50%;
  bottom: 40rpx;
  transform: translateX(-50%);
  text-align: center;
  padding-top: 35rpx;
  color: #fff;
  font-weight: normal;
}
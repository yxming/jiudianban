page {
  background-color: #F0F0F3; /* 设置背景颜色#f0f0f0; */
}

.backgroundD {
  width: calc(100%-30rpx);
  height: 100vh;
  padding: 0 30rpx 0 30rpx;
  position: relative;
}

/* 顶部导航 */
.shangmbubox{
  width: 100%;
  height: 90rpx;
  background-color: #F0F0F3;
  position: relative;
  top: 0;
  left: 0;
  padding-top: 95rpx;
}
.dohangind{
  width: 300rpx;
  height: auto;
  margin: auto;
  text-align: center;
  font-size: 40rpx;
  font-weight: 600;
}
.totalAmountMoney{
  width: 100%;
  height: auto;
  text-align: center;
  padding-top: 50rpx;
  font-size: 130rpx;
  color: #000;
}
.moneyBox{
  width: 300rpx;
  height: 200rpx;
  margin: auto;
}
.monetaryUnit{
  width: 426rpx;
  height: 50rpx;
  margin: auto;
  color: #A4A4A4;
  font-size: 32rpx;
}









/* 钱数 */
.serviceCharge {
  width: 100%;
  height: 300rpx;
  background-color: bisque;
  border-radius: 30rpx;
  overflow: hidden;
  margin: auto;
  position: relative;
}

.uptext {
  width: calc(100%-60rpx);
  height: 50rpx;
  position: absolute;
  top: 15rpx;
  left: 0;
  padding-left: 30rpx;
  padding-top: 10rpx;
  font-weight: normal;
  color: #fff;
}

.amountOfMoney {
  width: 500rpx;
  height: 150rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
}

.symbol {
  width: 30rpx;
  height: 50rpx;
  color: #fff;
  margin-top: 70rpx;
  font-size: 40rpx;
}

.symbolSu {
  width: 100%;
  height: auto;
  font-size: 100rpx;
  color: #fff;
}

/* 地址模块 */
.address {
  width: 100%;
  height: 230rpx;
  background-color: rgb(228, 228, 228);
  border-radius: 30rpx;
  margin: 30rpx 0 30rpx 0;
}

.pickUp {
  width: calc(100%-60rpx);
  height: 60rpx;
  padding-left: 30rpx;
  overflow: hidden;
  font-size: 30rpx;
  font-weight: normal;
  color: #000;
  padding-top: 15rpx;
}

.addressSelectionBox {
  width: calc(100%-60rpx);
  height: 60rpx;
  padding-left: 30rpx;
  display: flex;
  justify-content: space-between;
}

.addressbox {
  width: 30%;
  height: 100%;
  display: flex;
}

.addresstext {
  width: auto;
  height: 100%;
  font-size: 14rpx;
  color: #000;
  overflow: hidden;
  font-size: 30rpx;
  font-weight: normal;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.addressboxr {
  width: 70%;
  height: 100%;
  display: flex;
  flex-direction: row-reverse;
}

/* 这里是展示地址的  */
.addressDisplay {
  width: calc(100%-120rpx);
  height: 60rpx;
  margin: 0 60rpx 0 60rpx;
  padding-top: 10rpx;
  padding-left: 10rpx;
  overflow: hidden;
  font-size: 30rpx;
}

/* 我的家具模块 */
.myFurnitureBox {
  width: 100%;
  height: 260rpx;
  background-color: rgb(228, 228, 228);
  border-radius: 30rpx;
  margin: 30rpx 0 30rpx 0;
}

.specifications {
  width: 350rpx;
  height: 80rpx;
  background-color: brown;
}

/* 底部按钮 */
.bottomButton {
  width: 300rpx;
  height: 100rpx;
  background-color: brown;
  position: absolute;
  bottom: 60rpx;
  right: 30rpx;
  border-radius: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}

/* 模态框 */
.btn {
  width: 60%;
  padding: 20rpx 0;
  border-radius: 10rpx;
  text-align: center;
  margin: auto;
  margin-top: 15rpx;
  background: rgb(82, 99, 248);
  color: #fff;
  font-size: 30rpx;
  font-weight: normal;
}

/*mask*/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

/*content*/
.drawer_box {
  width: 650rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  background: #fff;
  margin: -150px 50rpx 0 50rpx;
  border-radius: 3px;
}

.drawer_title {
  padding: 15px;
  font: 20px "microsoft yahei";
  text-align: center;
}

.drawer_content {
  height: 210px;
  overflow-y: scroll;
  /*超出父盒子高度可滚动*/
}

.btn_ok {
  padding: 10px;
  font: 20px "microsoft yahei";
  text-align: center;
  border-top: 1px solid #E8E8EA;
  color: #3CC51F;
}

/* 模态框的输入框 */
.inputContent {
  width: calc(100%-60rpx);
  height: 100rpx;
  margin: 5rpx 30rpx;
  display: flex;
}

.inputContenttext {
  width: 150rpx;
  height: 50rpx;
  font-size: 30rpx;
  color: #000;
}

/* 底部弹出框 */
.block {
  width: 100vw;
  height: auto;
  background: #fff;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}

.wrapper {
  margin: 32rpx;
  display: block;
}

.header {
  display: flex;
  align-items: center;
  height: 116rpx;
}

.title {
  flex: 1;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
}

.btns {
  font-size: 32rpx;
  padding: 32rpx;
}

.btn--cancel {
  color: rgba(0, 0, 0, .6);
}

.btn--confirm {
  color: #0052d9;
}

/* 下拉框上部分盒子 */
.uoContentBox {
  width: calc(100%-60rpx);
  height: auto;
  margin: 5rpx 30rpx;
  overflow: hidden;
}

/* 下拉框滑动块样式 */
.scroll-x {
  width: 100%;
  height: auto;
  white-space: nowrap;
  display: flex;
  justify-content: space-around;
}

.acts {
  width: 200rpx;
  height: 230rpx;
  margin: 20rpx;
  padding-top: 20rpx;
  border-radius: 30rpx;
  overflow: hidden;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  background-color: rgb(219, 219, 219);
}

.zanshi {
  background-color: rgb(178, 247, 247);
  opacity: 0.5;
  color: rgb(55, 195, 238);
  border: slateblue solid 1px;
}

/* 块的样式 */
.activity_photo {
  width: 150rpx;
  height: 150rpx;
  margin: auto;
}

/* 块下面的字符样式 */
.electricalEquipmentText {
  width: 100%;
  height: auto;
  margin-top: 10rpx;
  font-size: 30rpx;
  text-align: center;
}

/* 手风琴样式 */
.container {
  width: 100%;
  height: 500rpx;
  overflow: hidden;
}

.content {
  width: calc(100%-60rpx);
  height: auto;
  margin: 5rpx 30rpx;
}

.list-content {
  display: flex;
  justify-content: space-between;
  text-align: center;
  line-height: 60rpx;
  border-bottom: 1rpx solid #ddd;
}

.list-text {
  padding-top: 15rpx;
  transition: all 0.5s ease;
  text-indent: 40rpx;
  /* 自动换行 */
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.hidden-content {
  display: none;
}

.optionFast {
  width: 180rpx;
  height: 90rpx;
  background-color: rgb(212, 212, 212);
  margin: 20rpx 20rpx 0 0;
  padding-left: 30rpx;
  padding-top: 30rpx;
  border-radius: 30rpx;
  font-size: large;
}
.selected{
  width: 180rpx;
  height: 90rpx;
  background-color: rgb(96, 197, 223);
  margin: 20rpx 20rpx 0 0;
  padding-left: 30rpx;
  padding-top: 30rpx;
  border-radius: 30rpx;
  font-size: large;
  color: #fff;
}



/* 地址选择 */
.selection {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
}

.Multiline {
  width: calc(100%-60rpx);
  height: 256rpx;
  margin: 20rpx 30rpx 0 30rpx;
  display: flex;
}

.external-class {
  width: 450rpx;
  height: 200rpx;
}

/* 三级联动 */
.input {
  width: 500rpx;
  height: 100rpx;
  line-height: 80rpx;
  background-color: rgb(255, 255, 255);
  padding-left: 30rpx;
  box-sizing: border-box;
}

.selectiontext {
  width: 150rpx;
  height: 80rpx;
  margin: 20rpx 0 0 30rpx;
}

.box {
  position: absolute;
  left: 0;
  bottom: 0;
  overflow: hidden;
  width: 100%;
  height: 100vh;
  z-index: 8;
}

.black {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, .5);
  z-index: 9000;
}

.nr {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 500rpx;
  z-index: 9001;
  background: #FFF;
}

.item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EEE;
}